{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": ["src/**/*", "../server/src"],
  "exclude": ["node_modules/*", "__sapper__/*", "public/*"],
  "moduleResolution": "node",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": ["src/*"],
      "@server/*": ["*"]
    },
    "types": ["svelte"],
    "target": "es2019"
  }
}
